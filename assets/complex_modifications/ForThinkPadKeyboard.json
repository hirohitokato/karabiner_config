        {
          "from": {
            "key_code": "page_up",
            "modifiers": {
              "mandatory": [
              	"option"
              ]
            }
          },
          "to": [
            {
              "key_code": "f14"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "page_down",
            "modifiers": {
              "mandatory": [
              	"option"
              ]
            }
          },
          "to": [
            {
              "key_code": "f15"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
